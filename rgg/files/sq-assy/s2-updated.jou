{include("s2.template-updated.jou")}
# Import geometry file 
import 's2.sat'
#merge geometry
merge all
#Creating curve sidesets, Note: you might need to change @ extensions
group 'tmpgrp' equals curve name "side_edge1"
sideset 1 curve in tmpgrp
group 'tmpgrp' equals curve name "side_edge2"
sideset 2 curve in tmpgrp
group 'tmpgrp' equals curve name "side_edge3"
sideset 3 curve in tmpgrp
group 'tmpgrp' equals curve name "side_edge4"
sideset 4 curve in tmpgrp
#Creating top surface sidesets
group 'tmpgrp' equals surface name "mat_block_top"
sideset 5 surface in tmpgrp
group 'tmpgrp' equals surface name "mat_coolant_top"
sideset 6 surface in tmpgrp
group 'tmpgrp' equals surface name "pwr_rf_r_01_top"
sideset 7 surface in tmpgrp
#Creating top surface sidesets
group 'tmpgrp' equals surface name "mat_block_bot"
sideset 8 surface in tmpgrp
group 'tmpgrp' equals surface name "mat_coolant_bot"
sideset 9 surface in tmpgrp
group 'tmpgrp' equals surface name "pwr_rf_r_01_bot"
sideset 10 surface in tmpgrp
group "g_mat_block" add body name "mat_block"
group "g_mat_coolant" add body name "mat_coolant"
group "g_pwr_rf_r_01" add body name "pwr_rf_r_01"
#Creating blocks, Note: you might need to combine some blocks
block 1 body in g_mat_block
block 1 name "b_mat_block"
block 2 body in g_mat_coolant
block 2 name "b_mat_coolant"
block 3 body in g_pwr_rf_r_01
block 3 name "b_pwr_rf_r_01"
#Set Meshing Scheme and Sizes, use template.jou to specify sizes
surface with z_coord > {-Z_MID +.1*Z_HEIGHT} and z_coord < {Z_MID - .1*Z_HEIGHT} size {AXIAL_MESH_SIZE}

vol in body in g_mat_block size  {mat_block_size}
vol in body in g_mat_block scheme {SWEEP}
mesh vol in body in g_mat_block
#
vol in body in g_mat_coolant size  {mat_coolant_size}
vol in body in g_mat_coolant scheme {SWEEP}
mesh vol in body in g_mat_coolant
#
vol in body in g_pwr_rf_r_01 size  {pwr_rf_r_01_size}
vol in body in g_pwr_rf_r_01 scheme {SWEEP}
mesh vol in body in g_pwr_rf_r_01
