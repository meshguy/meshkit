##
## This makefile is automatically generated by coregen program
##
## Check your coregen, assygen and cubit location
##
include ../../common.mk


CUBIT = trelis -nographics

COREGEN = $(MESHKIT_DIR)/bin/coregen

ASSYGEN = $(MESHKIT_DIR)/bin/assygen

MESH_FILES = a1.cub  a2.cub  a3.cub  

GEOM_FILES = a1.sat  a2.sat  a3.sat  

JOU_FILES = a1.jou  a2.jou  a3.jou  

PYTHON_FILES = a1.py  a2.py  a3.py  

INJOU_FILES = a1.template.jou  a2.template.jou  a3.template.jou  

ASSYGEN_FILES = a1.inp  a2.inp  a3.inp  

twelfth_hexflatcore.h5m : ${MESH_FILES} twelfth_hexflatcore.inp
	${COREGEN} twelfth_hexflatcore
a1.cub : a1.sat  a1.jou  a1.template.jou
	${CUBIT} -batch a1.jou

a1.sat : a1.py
	${CUBIT} -batch a1.py

a1.py a1.jou a1.template.jou : a1.inp
	${ASSYGEN} a1

a2.cub : a2.sat  a2.jou  a2.template.jou
	${CUBIT} -batch a2.jou

a2.sat : a2.py
	${CUBIT} -batch a2.py

a2.py a2.jou a2.template.jou : a2.inp
	${ASSYGEN} a2

a3.cub : a3.sat  a3.jou  a3.template.jou
	${CUBIT} -batch a3.jou

a3.sat : a3.py
	${CUBIT} -batch a3.py

a3.py a3.jou a3.template.jou : a3.inp
	${ASSYGEN} a3

make clean :
	rm *.sat *.cub *.py *.jou
